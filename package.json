{
  "name": "oracle-async",
  "version": "1.3.1",
  "description": "A wrapper for oracle for added convenience when working with async/await and inside docker containers.",
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib-esm/index.js"
    },
    "./db": {
      "types": "./lib-esm/db.d.ts",
      "require": "./lib/db.js",
      "import": "./lib-esm/db.js"
    }
  },
  "types": "./lib-esm/index.d.ts",
  "typesVersions": {
    "*": {
      "db": ["./lib-esm/db.d.ts"]
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib && tsc && mv lib/*.d.ts lib-esm/",
    "mocha": "mocha -r ts-node/register test/**/*.ts --exit",
    "test": "./test.sh"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.14",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "mocha": "^9.1.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/oracledb": "^5.0.0",
    "oracledb": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/oracle-async.git"
  },
  "keywords": [
    "oracle",
    "await",
    "async"
  ],
  "author": "Nick Wing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/txstate-etc/oracle-async/issues"
  },
  "homepage": "https://github.com/txstate-etc/oracle-async#readme",
  "files": [
    "lib",
    "lib-esm"
  ]
}
